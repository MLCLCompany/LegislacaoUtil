<?xml version='1.0' encoding='utf-8'?>
<widget id="com.CompanyMLCL.LegislacaoUtil" version="1.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Legislação Útil</name>
    <description>
              Legislação de bolso útil.
    </description>
    <author email="company.mlcl@gmail.com">
        Company MLCL
    </author>
    <icon gap:platform="android" gap:qualifier="ldpi" src="www/res/icon/android/mipmap-ldpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="mdpi" src="www/res/icon/android/mipmap-mdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="hdpi" src="www/res/icon/android/mipmap-hdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="xhdpi" src="www/res/icon/android/mipmap-xhdpi/ic_launcher.png" />
    <icon gap:platform="android" gap:qualifier="xxhdpi" src="www/res/icon/android/mipmap-xxhdpi/ic_launcher.png" />
    <content src="index.html" />
    <gap:plugin name="cordova-plugin-whitelist" source="npm" version="1.1.0" />
    <access launch-external="yes" origin="*" />
    <preference name="permissions" value="none" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
</widget>
